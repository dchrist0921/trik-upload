<Window x:Class="Trik.Upload_Extension.PropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PropertiesWindow" Height="116" Width="216" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">

    <Grid Margin="-2,0" Height="81" VerticalAlignment="Top">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Margin="10,46,9,10" Click="ButtonOkay_Click" Content="OK" Grid.RowSpan="2" />
        <Button Margin="7,46,10,10" Content="Cancel" Click="ButtonClose_OnClicked" Grid.Row="0" Grid.RowSpan="2" Grid.Column="1" />
        <ComboBox x:Name="ComboBox" Margin="10,10,10,0" Grid.ColumnSpan="2" Grid.Column="0" Grid.Row="0" Grid.RowSpan="2" Height="25" VerticalAlignment="Top" />
    </Grid>

</Window>